  "entries": {
    "server": {
      "driver": "local_storage",
      "options": {
        "root": "server"
      }
    },
    "dropbox": {
      "driver": "dropbox",
      "options": {
        "root": "/onitu/",
        "access_key": "1n8rl2za664hf6q6",
        "access_secret": "lvw0p4e93tbfgze"
      }
    },
    "hubic": {
      "driver": "hubic",
      "options": {
        "root": "/onitu",
          "refresh_token": "ztEaw0hidSMDYidpoPn4UUueJDFewh2UJFeoM7i8DeHTkXLusMj868lqESUsRVDI",
          "changes_timer" : 10
      }
    },
    "flickr": {
	"driver": "flickr",
	"options": {
	    "root": "album-onitu",
	    "oauth_token": "72157648955146886-9f3dbcb0a5ad79b7",
	    "oauth_token_secret": "b691556beabdc4a6"
	}
    }
  },

  "rules": [
    {
      "match": {"path": "/Musique/", "mime": ["audio/mpeg"]},
      "sync": ["server", "dropbox", "hubic", "remote-1"]
    },
    {
      "match": {"path": "/Documents/", "mime": ["application/pdf",
						"application/vnd.openxmlformats-officedocument.presentationml.presentation",
					       "application/vnd.ms-word",
					       "application/vnd.oasis.opendocument.text",
					       "application/vnd.oasis.opendocument.spreadsheet"
						]},
      "sync": ["server", "dropbox", "hubic", "remote-1"]
    },
    {
      "match": {"path": "/Photos/", "mime": ["image/jpeg"]},
      "sync": ["server", "flickr", "remote-1"]
    }
  ]
}
